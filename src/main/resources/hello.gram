#JSGF V1.0;

/**
 * JSGF Grammar for Hello World example
 */

grammar hello;

public <basicCmd> = <command> <endPolite>;
<command> = <action> <object>;
<action> = (turn on | turn off | turn up | turn down | save | get);
<object> = [my | Matt | Dan] (light one| light two | preferences);
<endPolite> = [thanks | thank you | please];
